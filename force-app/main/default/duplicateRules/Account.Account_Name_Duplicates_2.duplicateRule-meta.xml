<?xml version="1.0" encoding="UTF-8"?>
<DuplicateRule xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <actionOnInsert>Allow</actionOnInsert>
    <actionOnUpdate>Allow</actionOnUpdate>
    <alertText>We found some other organisations with similar names. Use one of these?</alertText>
    <description>Warn users when similar account names with existing accounts are detected</description>
    <duplicateRuleFilter>
        <booleanFilter>2 AND 4 AND  (1 OR 3 OR 5 OR 6 OR 7)</booleanFilter>
        <duplicateRuleFilterItems>
            <field>Profile_Name_in_Text__c</field>
            <operation>equals</operation>
            <value>Customer Team</value>
            <sortOrder>1</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>Profile_Name_in_Text__c</field>
            <operation>equals</operation>
            <value>Product Sales</value>
            <sortOrder>2</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>IsPersonAccount</field>
            <operation>equals</operation>
            <value>false</value>
            <sortOrder>3</sortOrder>
            <table>Account</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>Profile_Name_in_Text__c</field>
            <operation>equals</operation>
            <value>Hub Users</value>
            <sortOrder>4</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>Profile_Name_in_Text__c</field>
            <operation>equals</operation>
            <value>Credit Management</value>
            <sortOrder>5</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>Profile_Name_in_Text__c</field>
            <operation>equals</operation>
            <value>PT1</value>
            <sortOrder>6</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>RecordType</field>
            <operation>equals</operation>
            <value>Organisation</value>
            <sortOrder>7</sortOrder>
            <table>Account</table>
        </duplicateRuleFilterItems>
    </duplicateRuleFilter>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Account</matchRuleSObjectType>
        <matchingRule>Account_Name_Matching_Rule</matchingRule>
        <objectMapping xsi:nil="true"/>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>Account Name Duplicates 2</masterLabel>
    <operationsOnInsert>Alert</operationsOnInsert>
    <operationsOnInsert>Report</operationsOnInsert>
    <operationsOnUpdate>Alert</operationsOnUpdate>
    <operationsOnUpdate>Report</operationsOnUpdate>
    <securityOption>EnforceSharingRules</securityOption>
    <sortOrder>4</sortOrder>
</DuplicateRule>
