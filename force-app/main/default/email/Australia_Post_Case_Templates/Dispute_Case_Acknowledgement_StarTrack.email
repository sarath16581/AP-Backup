  /* custom device helpers*/
  @media screen and (max-device-width: 619px), screen and (max-width: 619px) {
    .size-11 { font-size: 11px !important; line-height: 14px !important; }
    .m-snap-right, .m-text-left { text-align: left !important; }
  }

  .corners {border-radius: 4px;}
  .corners-top {border-radius: 4px 4px 0 0;}
  .corners-bottom {border-radius: 0 0 4px 4px;}

  /* main layout CSS for web email clients*/
  .layout,
  .column {
    transition: width 0.25s ease-in-out, max-width 0.25s ease-in-out;
  }
  .layout,
  .header {
    max-width: 400px !important;
    -fallback-width: 95% !important;
    width: calc(100% - 20px) !important;
  }
  .column {
    max-width: 400px !important;
    width: 100% !important;
  }
  .snippet,
  .webversion {
    width: 50% !important;
  }
  .ie .btn {
    width: 100%;
  }
  .ie .layout,
  [owa] .layout,
  .ie .one-col .column,
  [owa] .one-col .column {
    max-width: 600px !important;
    width: 600px !important;
  }
  .ie .fixed-width.has-border,
  [owa] .fixed-width.has-border,
  .ie .has-gutter.has-border,
  [owa] .has-gutter.has-border {
    max-width: 602px !important;
    width: 602px !important;
  }
  .ie .two-col .column,
  [owa] .two-col .column {
    max-width: 300px !important;
    width: 300px !important;
  }
  .ie .three-col .column,
  [owa] .three-col .column,
  .ie .narrow,
  [owa] .narrow {
    max-width: 150px !important;
    width: 150px !important;
  }
