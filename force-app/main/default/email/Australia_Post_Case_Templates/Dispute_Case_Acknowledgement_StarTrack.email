  <style type="text/css">
  body {
    margin: 0;
    padding: 0;
  }
  table {
    border-collapse: collapse;
    table-layout: fixed;
  }
  * {
    line-height: inherit;
  }
  [x-apple-data-detectors],
  [href^="tel"],
  [href^="sms"] {
    color: inherit !important;
    text-decoration: none !important;
  }

  a:hover,.btn a:hover,
  a.btn img:hover,
  a img:hover,
  .btn a:focus {
    opacity: 0.8 !important;
  }

  /* brand colours*/
  .cream  {color: #f3f1ee;}   /* background colour*/
  .white  {color: #ffffff;}  /* content background colour*/
  .brown  {color:#5e514d;}   /* body text colour*/
  .blue   {color: #37a0dc;}  /* ST Brand Heading*/
