  <style type="text/css">
  body {
    margin: 0;
    padding: 0;
  }
  table {
    border-collapse: collapse;
    table-layout: fixed;
  }
  * {
    line-height: inherit;
  }
  [x-apple-data-detectors],
  [href^="tel"],
  [href^="sms"] {
    color: inherit !important;
    text-decoration: none !important;
  }

  a:hover,.btn a:hover,
  a.btn img:hover,
  a img:hover,
  .btn a:focus {
    opacity: 0.8 !important;
  }

  /* brand colours*/
  .cream  {color: #f3f1ee;}   /* background colour*/
  .white  {color: #ffffff;}  /* content background colour*/
  .brown  {color:#5e514d;}   /* body text colour*/
  .blue   {color: #37a0dc;}  /* ST Brand Heading*/
  .blue-dark {color: #18376b;} /* ST Text Links*/
  .dark-brown {color: #382f2d } /* Brand Heading*/


  /* custom device helpers*/
  @media screen and (max-device-width: 619px), screen and (max-width: 619px) {
    .size-11 { font-size: 11px !important; line-height: 14px !important; }
    .m-snap-right, .m-text-left { text-align: left !important; }
  }

  .corners {border-radius: 4px;}
  .corners-top {border-radius: 4px 4px 0 0;}
  .corners-bottom {border-radius: 0 0 4px 4px;}

  /* main layout CSS for web email clients*/
  .layout,
  .column {
    transition: width 0.25s ease-in-out, max-width 0.25s ease-in-out;
  }
  .layout,
  .header {
    max-width: 400px !important;
    -fallback-width: 95% !important;
    width: calc(100% - 20px) !important;
  }
  .column {
    max-width: 400px !important;
    width: 100% !important;
  }
  .snippet,
  .webversion {
    width: 50% !important;
  }
  .ie .btn {
    width: 100%;
  }
  .ie .layout,
  [owa] .layout,
  .ie .one-col .column,
  [owa] .one-col .column {
    max-width: 600px !important;
    width: 600px !important;
  }
  .ie .fixed-width.has-border,
  [owa] .fixed-width.has-border,
  .ie .has-gutter.has-border,
  [owa] .has-gutter.has-border {
    max-width: 602px !important;
    width: 602px !important;
  }
  .ie .two-col .column,
  [owa] .two-col .column {
    max-width: 300px !important;
    width: 300px !important;
  }
  .ie .three-col .column,
  [owa] .three-col .column,
  .ie .narrow,
  [owa] .narrow {
    max-width: 150px !important;
    width: 150px !important;
  }
  .ie .wide,
  [owa] .wide {
    width: 400px !important;
  }
  .ie .fixed-width .layout__inner {
    border-left: 0 none white !important;
    border-right: 0 none white !important;
  }
  .layout-fixed-width,
  .mso .layout-full-width {
    background-color: #ffffff;
  }
  @media only screen and (min-width: 620px) {
    .column {
      display: table-cell;
      Float: none !important;
      vertical-align: top;
    }
    .layout,
    .one-col .column {
      max-width: 600px !important;
      width: 600px !important;
    }
    .two-col .column {
      max-width: 300px !important;
      width: 300px !important;
    }
    .three-col .column,
    .column.narrow {
      max-width: 150px !important;
      width: 150px !important;
    }
    .column.wide {
      width: 400px !important;
    }
  }

  @media (max-width: 321px) {
    .layout,
    .column {
      min-width: 320px !important;
      width: 320px !important;
    }
  }

</style>

