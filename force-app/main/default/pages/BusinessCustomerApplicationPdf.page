                </div>
              </div>
            </apex:outputPanel>        
        
            <!-- DO NOT display this section when parcel is less than 500 -->
            <apex:outputPanel id="cred-acc-6" styleClass="input input-columns checkbox" rendered="{!appData.parcelsPerYear = '500 - 999'}">
                <p>Would you like to apply for the eParcel service?</p>
                <apex:outputText value="{!appData.eParcelApplication}" styleClass="FormOutputText"  id="cred-acc-6-textbox"/>
            </apex:outputPanel>
            <!-- end -->                 

