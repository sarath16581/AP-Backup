<h2>Australia Post Business Credit Account and parcel volume requirements</h2>
            <p>These questions will help us determine which Australia Post business products can best server your needs.</p>
            <h3>Australia Post Business Credit Account details</h3>
            <apex:outputPanel id="cred-acc-1" styleClass="checkbox input required input-columns">
              <p>Are you an existing Australia Post Business Credit Account customer?</p>
              <apex:outputText value="{!existingCreditAccount}" styleClass="FormOutputText" id="cred-acc-1-textbox"/>
            </apex:outputPanel>
            
            <!-- display this section when it is an Australia Post credit account customer -->
            <apex:outputPanel id="cred-acc-2" rendered="{!existingCreditAccount = 'Yes'}">                
               <label for="credit-account-number">Australia Post Business Credit Account number</label>
               <div class="input-medium">
                  <apex:outputText value="{!appData.creditAccountNumber}" styleClass="FormOutputText"  id="credit-account-number"/>
               </div>
            </apex:outputPanel>
            <!-- end -->
            <apex:outputPanel id="cred-acc-7" styleClass="input input-small" rendered="{!existingCreditAccount = 'Yes'}">
                <label>Do you currently lodge articles over the counter?</label>
                <apex:outputtext value="{!appData.lodgeOverTheCounter}" styleClass="FormOutputText" />
            </apex:outputPanel>
            <apex:outputPanel id="cred-acc-8" styleClass="input input-large" rendered="{!existingCreditAccount = 'Yes'}">
                <label>Which postal outlet do you currently lodge at?</label>
                <apex:outputtext id="lodge-post-outlet" value="{!appData.lodgePostOutlet}" styleClass="FormOutputText" />
            </apex:outputPanel>
            
            <!-- display this section when it is not an Australia Post credit account customer -->
            <apex:outputPanel id="cred-acc-3"  styleClass="input required input-columns checkbox" rendered="{!existingCreditAccount = 'No'}">
              <p>Would you like to apply for an Australia Post Business Credit Account?</p>
              <apex:outputText value="{!applyForCreditAccount}" styleClass="FormOutputText"  id="cred-acc-3-textbox"/>
            </apex:outputPanel>
            <!-- end -->
            
            <!-- display this section when customer would like to apply for an Australia Post credit account -->
            <apex:outputPanel id="cred-acc-4" styleClass="input required input-columns checkbox" rendered="{!existingCreditAccount = 'No' && (existingCreditAccount = 'Yes' || applyForCreditAccount = 'Yes')}">
              <p>Do you require a credit limit greater than $5000?</p>
              <apex:outputText value="{!appData.creditLimit5000}" styleClass="FormOutputText"  id="cred-acc-4-textbox"/>
            </apex:outputPanel>
            <!-- end -->
          
