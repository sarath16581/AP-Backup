                            <br/><br/>
                            <apex:outputPanel rendered="{!configSetting.DisplayAlert__c}">
                                <div class="tempAlert">
                                    <h3>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
