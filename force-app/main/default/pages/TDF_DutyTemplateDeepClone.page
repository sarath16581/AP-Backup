<apex:page standardController="Route_Template__c" extensions="TDF_DutyTemplateDeepCloneControllerX">
	<apex:pageMessages id="pgMsg"/>
	<apex:form >
		<apex:pageBlock >
			<apex:pageBlockSection columns="1">			
				<apex:inputText value="{!dutyTemplateName}" id="dtName" style="width:25%" label="Duty Template Name"/>
			</apex:pageBlockSection>
			<apex:pageBlockButtons >
				<apex:commandButton value="Clone" action="{!deepCloneDutyTemplate}"/>
				<apex:commandButton value="Cancel" action="{!cancel}"/>
			</apex:pageBlockButtons>
		</apex:pageBlock>
	</apex:form>
</apex:page>