    <sharingCriteriaRules>
        <fullName>All_Internal_Users_except_Retail_Merchandise</fullName>
        <accessLevel>Edit</accessLevel>
        <accountSettings>
            <caseAccessLevel>None</caseAccessLevel>
            <contactAccessLevel>Edit</contactAccessLevel>
            <opportunityAccessLevel>Read</opportunityAccessLevel>
        </accountSettings>
        <description>This rule enables all accounts to be shared within internal users except retail merchandise &amp; CC users who only need access retail data.</description>
        <label>All Internal Users except Retail Merchandise &amp; CC</label>
        <sharedTo>
            <group>All_Users_Ex_Retail_Merchandise_CC</group>
        </sharedTo>
        <criteriaItems>
            <field>Is_a_facility__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
