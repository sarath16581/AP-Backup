    <sharingCriteriaRules>
        <fullName>DSR_Customer_onboarding</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share Customer onboarding record type DSR with all users with Role- Customer Data Team</description>
        <label>DSR Customer onboarding</label>
        <sharedTo>
            <role>CustomerDataManagementTeam</role>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Customer Onboarding</value>
        </criteriaItems>
    </sharingCriteriaRules>
