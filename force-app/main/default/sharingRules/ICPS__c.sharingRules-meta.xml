<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>NSW_Users_see_all_NSW_records</fullName>
        <accessLevel>Edit</accessLevel>
        <description>All NSW ICPS users can view/edit all NSW ICPS records</description>
        <label>NSW Users see all NSW records</label>
        <sharedTo>
            <role>ICPSNSW</role>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>NSW</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
