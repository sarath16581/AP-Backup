<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Dashcord_Sharing_Rule</fullName>
        <accessLevel>Edit</accessLevel>
        <description>used by Dashcord app</description>
        <label>Dashcord Sharing Rule</label>
        <sharedTo>
            <group>Dashcord</group>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>notEqual</operation>
            <value>.</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>Campaign_Contact_Assignment_Sales_Managers</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Setup to ensure campaigns created by Marketing users can be edited by Sales Managers</description>
        <label>Campaign Contact Assignment Sales Managers</label>
        <sharedTo>
            <group>Account_Managers</group>
        </sharedTo>
        <sharedFrom>
            <role>Marketing</role>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Campaign_Contact_Assignments_Sales</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Designed to share Campaign Read/Write access to Sales Managers when created by Marketing users.</description>
        <label>Campaign Contact Assignments Sales</label>
        <sharedTo>
            <group>Sales_Managers</group>
        </sharedTo>
        <sharedFrom>
            <role>Marketing</role>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Campaign_Contact_Assingments_Marketing</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Setup to allow Campaigns to be edited by Marketing users when created by other Marketing users</description>
        <label>Campaign Contact Assingments Marketing</label>
        <sharedTo>
            <role>Marketing</role>
        </sharedTo>
        <sharedFrom>
            <role>Marketing</role>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
