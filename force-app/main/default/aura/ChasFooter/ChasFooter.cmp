<aura:component implements="forceCommunity:availableForAllPageTypes" description="Footer links">
    <!-- Added below code on 18/10/2018 for Header and Footer changes for mobile App. -->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="status" type="String" />
    <!-- Added below if condition on 18/10/2018 to show or hide Header and Footer. -->
    <aura:if isTrue="{!v.status == 'show' ? true : false}">
	<div>
		<div class="container">
			<div class="section-one">
				<a data-url="https://auspost.com.au/" onclick="{! c.onclick }">Auspost.com.au</a>
				<a data-url="https://auspost.com.au/about-us" onclick="{! c.onclick }">About us</a>
				<a data-url="https://auspost.com.au/privacy" onclick="{! c.onclick }">Privacy policy</a>
				<a data-url="https://auspost.com.au/terms-conditions" onclick="{! c.onclick }">Terms &amp; conditions</a>
				<a data-url="https://auspost.com.au/about-us/about-our-site/accessibility" onclick="{! c.onclick }">Accessibility</a>
			</div>
			<div class="section-two">
				<a data-url="https://ptest.npe.auspost.com.au/help-and-support" onclick="{! c.onclick }"><img src="{! $Resource.ChasIcons + '/chas-icons/svgs/UI/icons/hf_help_support.svg' }" alt="" width="22" height="22" class="slds-m-right_x-small" />Help &amp; support</a>
			</div>
		</div>
	</div>
    </aura:if>
</aura:component>

		
