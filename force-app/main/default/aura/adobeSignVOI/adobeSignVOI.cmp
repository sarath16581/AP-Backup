<aura:component controller="AdobeSignCustomController" implements="lightning:isUrlAddressable" description="c:adobeSignVOI component">
	<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
	<aura:attribute name="masterId" type="String" />
	<aura:attribute name="attachmentIds" type="String" />
	<lightning:formattedText value="Generating Adobe Sign Envelope..." />
	<lightning:spinner class="spins" size="large" variant="brand" alternativeText="Loading" />
</aura:component>	