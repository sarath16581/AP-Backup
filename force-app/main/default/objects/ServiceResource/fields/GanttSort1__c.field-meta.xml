<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GanttSort1__c</fullName>
    <description>Field to add to available sort options in the gantt view. Currently by Duty (sorted by start time) followed by Driver (sorted by name)</description>
    <externalId>false</externalId>
    <formula>IF (TEXT(ResourceType) = &apos;C&apos;, &apos;0 - &apos; + TEXT( DutyStartTime__c ) + &apos; &apos; + Name, Name)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Duty &gt; Driver</label>
    <required>false</required>
    <securityClassification>OFFICIAL_SENSITIVE_PERSONAL</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
