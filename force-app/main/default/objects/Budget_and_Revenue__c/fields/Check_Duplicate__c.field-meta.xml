<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Duplicate__c</fullName>
    <description>Check if this record is a duplicate or not</description>
    <externalId>false</externalId>
    <formula>If (Value(LEFT( Unique_Id__c , 8))= Organisation__r.Customer_Number__c, &quot;Not Duplicate&quot;, &quot;Duplicate&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Check_Duplicate</label>
    <required>false</required>
    <securityClassification>INTERNAL</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
