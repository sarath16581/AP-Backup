<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TotalLicencesAvailableToUse__c</fullName>
    <externalId>false</externalId>
    <formula>IF(TotalLicencesAttachedToThisOrder__c - TotalLicencesPurchased__c &lt; 1, TotalLicencesAttachedToThisOrder__c -  TotalLicencesUsed__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This is a formula of the total licences attached to this order minus the total licences used.</inlineHelpText>
    <label>TotalLicencesAvailableToUse</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
