<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMAPP__Any_Overdue_Actions__c</fullName>
    <deprecated>false</deprecated>
    <description>This checkbox is selected if there are any open actions whose due date is earlier than today. Otherwise, it is deselected.</description>
    <externalId>false</externalId>
    <formula>IF(DMAPP__Minimum_Due_Date_Of_Open_Actions__c &lt; TODAY(), True, False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Any Overdue Actions</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
