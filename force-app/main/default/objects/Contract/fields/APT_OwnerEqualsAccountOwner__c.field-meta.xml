<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>APT_OwnerEqualsAccountOwner__c</fullName>
    <description>This formula field returns false if the contract owner is not equal to related account owner</description>
    <formula>IF(OwnerId  &lt;&gt; Account__r.OwnerId , false, true)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>OwnerEqualsAccountOwner</label>
    <securityClassification>INTERNAL</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
