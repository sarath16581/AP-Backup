<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Score_Stars_del__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Hidden_Data_Score__c &gt;=80, IMAGE(&quot;/img/samples/flag_green.gif&quot;, &quot;green flag&quot;), IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;red flag&quot;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field is determined by the Data Quality Score and will show one of three values: 0-60 = Red Flag, 60-80 = Amber Flag, 80-100 = Green Flag</inlineHelpText>
    <label>Data Quality Rating</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
