<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Secondary_Spend</fullName>
    <active>true</active>
    <description>Secondary Spend must be less than Total Spend</description>
    <errorConditionFormula>Secondary_Competitor_Spend_FY__c &gt;   Total_estimated_spend_on_this_Service__c</errorConditionFormula>
    <errorDisplayField>Secondary_Competitor_Spend_FY__c</errorDisplayField>
    <errorMessage>Secondary Competitor Spend must be less than Total Spend</errorMessage>
</ValidationRule>
