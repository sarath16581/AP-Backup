<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CurrentYear__c</fullName>
    <description>Current year flag checked for this financial year</description>
    <formula>IF(Current_Last_Year__c=&apos;THIS YEAR&apos;, true,false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>True when current year is this FY</inlineHelpText>
    <label>Current-Year</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
