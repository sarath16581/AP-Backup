<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Implementation_Work_Type__c</fullName>
    <description>This field is to determine if a Sale implementation case is related to opportunity or not</description>
    <externalId>false</externalId>
    <formula>IF(  ISBLANK(Opportunity__c ) , &apos;Non-Opportunity Work&apos;, &apos;Opportunity Work&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Implementation Work Type</label>
    <required>false</required>
    <securityClassification>INTERNAL</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
