<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Overall_Time_Estimate__c</fullName>
    <description>This field will show the total time to build the requirement.</description>
    <externalId>false</externalId>
    <formula>Time_to_Investigate__c +Time_to_Build__c +  Time_to_Test__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Overall Time Estimate (mins)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>INTERNAL</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
