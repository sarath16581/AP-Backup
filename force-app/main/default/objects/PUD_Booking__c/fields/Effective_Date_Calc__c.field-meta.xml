<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Effective_Date_Calc__c</fullName>
    <description>Calculated effective date to support deriving date from parent etc</description>
    <formula>IF(ISBLANK(Parent_Booking__c),Effective_Date__c,Parent_Booking__r.Effective_Date__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Effective Date Calc</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
