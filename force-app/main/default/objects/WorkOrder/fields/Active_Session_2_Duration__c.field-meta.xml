<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Session_2_Duration__c</fullName>
    <description>The second active session for the duty. Used to determine if there has been a rest break violation in field Rest_Break_Violation.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Rest_Break_2_Start__c)),
 (Rest_Break_2_Start__c - Rest_Break_1_End__c)*24,
 IF(AND(NOT(ISBLANK(Rest_Break_1_End__c)),NOT(ISBLANK(Duty_End__c))),
  (Duty_End__c - Rest_Break_1_End__c)*24,
  NULL
))</formula>
    <label>Active Session 2 Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
