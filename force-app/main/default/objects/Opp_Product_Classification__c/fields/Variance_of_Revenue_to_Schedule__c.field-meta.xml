<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Variance_of_Revenue_to_Schedule__c</fullName>
    <description>This field will show the variance between the actual revenue and the scheduled revenue.</description>
    <formula>Total_Actual_Revenue__c -  Total_Schedule_Amount__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Variance of Revenue to Schedule</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <securityClassification>OFFICIAL_SENSITIVE</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
