<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TotalRetained__c</fullName>
    <description>Total Retained derived from Total Price</description>
    <externalId>false</externalId>
    <formula>IF(  RetainedRevenue__c = 0, 0,  (RetainedRevenue__c / Annualised_Value__c ) *  TotalPrice  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total Retained derived from Total Price</inlineHelpText>
    <label>Total Retained</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <securityClassification>INTERNAL</securityClassification>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
