<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TotalIncremental__c</fullName>
    <description>Total increment derived from Total Price</description>
    <externalId>false</externalId>
    <formula>IF(  IncrementalRevenue__c = 0, 0, (IncrementalRevenue__c / Annualised_Value__c) * TotalPrice )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total increment derived from Total Price</inlineHelpText>
    <label>Total Incremental</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <securityClassification>INTERNAL</securityClassification>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
