<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FED__c</fullName>
    <description>Fiscal End Date</description>
    <formula>IF( Close_Date__c  &lt; DATE(YEAR(Close_Date__c),7,1) 
,DATE(YEAR(Close_Date__c),6,30) 
,DATE(YEAR(Close_Date__c)+1,6,30) 
)</formula>
    <label>Fiscal End Date</label>
    <required>false</required>
    <securityClassification>INTERNAL</securityClassification>
    <type>Date</type>
</CustomField>
