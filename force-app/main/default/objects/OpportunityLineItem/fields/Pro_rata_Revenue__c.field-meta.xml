<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pro_rata_Revenue__c</fullName>
    <formula>IF(ISPICKVAL( Opportunity.Type, &quot;New - One Off&quot;), TotalPrice, (TotalPrice / 365) *   Opportunity.EFY__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Pro-rata Revenue</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>OFFICIAL_SENSITIVE</securityClassification>
    <type>Currency</type>
</CustomField>
