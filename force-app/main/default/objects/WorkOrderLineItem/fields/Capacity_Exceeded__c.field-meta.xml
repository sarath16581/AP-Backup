<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Capacity_Exceeded__c</fullName>
    <description>Shows true if the quantity exceeds the capacity of the linked vehicle capacity. (Only applies to ULD-Full products)</description>
    <externalId>false</externalId>
    <formula>AND(  Product_Name__c = &quot;ULD - Full&quot;, Actual_Quantity__c &gt;  WorkOrder.Vehicle__r.Capacity__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Capacity Exceeded?</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
