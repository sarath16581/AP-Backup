<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMAPP__TargetedRelationshipCoverageValidRange</fullName>
    <active>true</active>
    <errorConditionFormula>(DMAPP__Strategic_Coverage_Green__c  &lt; 0 || DMAPP__Strategic_Coverage_Green__c &gt; 1) ||
(DMAPP__Strategic_Coverage_Amber__c &lt; 0 || DMAPP__Strategic_Coverage_Amber__c &gt; 1) ||
(DMAPP__Strategic_Coverage_Amber__c  &gt;  DMAPP__Strategic_Coverage_Green__c)</errorConditionFormula>
    <errorMessage>Targeted Relationship Coverage Green and Amber cannot be less than 0% or greater than 100% and Targeted Relationship Coverage Green cannot be less than Targeted Relationship Coverage Amber</errorMessage>
</ValidationRule>
