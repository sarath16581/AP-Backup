<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>hidden_I_Am_Co_Host__c</fullName>
    <description>This field returns True if the Co-Host ID matches the UserID. Used to set up the list view &quot;Client Events I Co-Host&quot; to filter where the value of this field = TRUE</description>
    <externalId>false</externalId>
    <formula>IF (Owner_2__r.Id = $User.Id , &quot;True&quot;, &quot;False&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>{hidden}I Am Co-Host</label>
    <required>false</required>
    <securityClassification>INTERNAL</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
