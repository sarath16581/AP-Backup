<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rev3__c</fullName>
    <description>This field calculates the revenue forecast for the month of March.</description>
    <externalId>false</externalId>
    <formula>OpportunityProduct__r.TotalPrice *  FCT3__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated revenue forecast for March.</inlineHelpText>
    <label>March Revenue</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <securityClassification>INTERNAL</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
