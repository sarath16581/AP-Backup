<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HasExtraCover__c</fullName>
    <description>Indicates if the related case article has extra cover</description>
    <externalId>false</externalId>
    <formula>Case__r.ArticleTest__r.InsuranceAmount__c &gt; 0 &amp;&amp; ISBLANK(Case__r.ArticleTest__r.MLID__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates if the related case article has extra cover</inlineHelpText>
    <label>Has Extra Cover?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
