<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_Cannot_Be_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;&apos;), OR(RecordType.DeveloperName = &apos;Security_Express&apos;, RecordType.DeveloperName = &apos;Special_Delivery&apos;))</errorConditionFormula>
    <errorDisplayField>Status__c</errorDisplayField>
    <errorMessage>Please select a status for the job.</errorMessage>
</ValidationRule>
