<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Job_Type_Verification</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISPICKVAL(Job_Type__c, &apos;Security Express&apos;)), NOT(ISPICKVAL(Job_Type__c, &apos;Special Services&apos;)))</errorConditionFormula>
    <errorDisplayField>Job_Type__c</errorDisplayField>
    <errorMessage>Job Type is invalid.</errorMessage>
</ValidationRule>
