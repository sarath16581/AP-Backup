<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>ICPSArticleOutboundMessagetoSAPEM</fullName>
        <apiVersion>54.0</apiVersion>
        <description>This outbound message is triggered whenever a new ICPS article record is created or whenever an existing ICPS record has it&apos;s status changed.</description>
        <endpointUrl>https://webapi-ptestext.npe.auspost.com.au/stest/internationalparcels/icps/v1/article/events</endpointUrl>
        <fields>EventCode__c</fields>
        <fields>ICPSReference__c</fields>
        <fields>Id</fields>
        <fields>LastModifiedDate</fields>
        <fields>Name</fields>
        <fields>SalesforceName__c</fields>
        <fields>SenderId__c</fields>
        <includeSessionId>true</includeSessionId>
        <integrationUser>apintegrationuser@auspost.com.au</integrationUser>
        <name>ICPS Article Outbound Message to SAP EM</name>
        <protected>false</protected>
        <useDeadLetterQueue>true</useDeadLetterQueue>
    </outboundMessages>
</Workflow>
