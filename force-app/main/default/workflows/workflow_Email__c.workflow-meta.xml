    <rules>
        <fullName>StarTrack Check Address Notification - Customer Service</fullName>
        <actions>
            <name>StarTrack_Check_Address_Notification_Customer_Service</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>workflow_Email__c.Workflow_Email_Alert__c</field>
            <operation>equals</operation>
            <value>StarTrack Check Address Notification - Customer Service</value>
        </criteriaItems>
        <criteriaItems>
            <field>workflow_Email__c.To_Address__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Workflow rule to send Check Address notification email to customer.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
