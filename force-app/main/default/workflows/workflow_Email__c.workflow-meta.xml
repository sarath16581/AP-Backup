    <rules>
        <fullName>StarTrack Notify Receiver Case Closed</fullName>
        <actions>
            <name>StarTrack_Notify_Receiver_Case_Closed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(     Workflow_Email_Alert__c = &apos;Notify Receiver Case Closed&apos;,     NOT(ISBLANK(To_Address__c)))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
