    <description>subflow for MyCustomerDomain triggerred from CaseCommon AfterSave flow</description>
    <environments>Default</environments>
    <formulas>
        <name>vCaseClosed</name>
        <dataType>Boolean</dataType>
        <expression>{!vCase.Permanent_Close__c} &amp;&amp; {!vCase.LastIncomingEmailAddress__c} &lt;&gt; {!vOldCase.LastIncomingEmailAddress__c} &amp;&amp; NOT(ISBLANK({!vCase.LastIncomingEmailAddress__c})) &amp;&amp; {!vCase.LastIncomingEmailAddress__c} &lt;&gt; {!$Setup.QM__c.CusEmail__c} &amp;&amp; {!vCase.LastIncomingEmailAddress__c} &lt;&gt; {!$Setup.QM__c.NetworkEmail__c}</expression>
    </formulas>
    <formulas>
        <name>vCaseClosedBSPBulkUpload</name>
        <dataType>Boolean</dataType>
        <expression>{!vCase.Subject} == &apos;Bulk Upload&apos; &amp;&amp; TEXT({!vCase.Origin}) = &apos;BSP&apos; &amp;&amp; NOT(ISBLANK({!vCase.Contact.Email})) &amp;&amp; ISBLANK({!vCase.ParentId}) &amp;&amp; {!vCase.IsClosed}</expression>
    </formulas>
    <formulas>
        <description>Automatic case closure by the exception checker</description>
        <name>vCaseClosedExceptionChecker</name>
        <dataType>Boolean</dataType>
        <expression>{!vCase.IsClosed} &amp;&amp; {!vCase.ClosedBySystem__c} &amp;&amp; {!vCase.ByPassWorkflowRulesOnCreation__c}</expression>
    </formulas>
    <formulas>
        <name>vNewACK</name>
        <dataType>Boolean</dataType>
        <expression>AND(ISPICKVAL({!vCase.Status}, &apos;New&apos;), {!vCase.Exclude_Notification_Automation__c} = false, ISPICKVAL({!vCase.Business_Unit__c}, &apos;Australia Post&apos;), NOT(ISPICKVAL({!vCase.ReasonforCreditClaim__c}, &apos;Late payment&apos;)))</expression>
    </formulas>
    <formulas>
        <name>vNewACKLatePayment</name>
        <dataType>Boolean</dataType>
        <expression>AND(ISPICKVAL({!vCase.Status}, &apos;New&apos;), {!vCase.Exclude_Notification_Automation__c} = false, OR(ISPICKVAL({!vCase.Business_Unit__c}, &apos;Australia Post&apos;), ISPICKVAL({!vCase.Business_Unit__c}, &apos;StarTrack&apos;)), ISPICKVAL({!vCase.Enquiry_Type__c}, &apos;Billing Dispute&apos;), ISPICKVAL({!vCase.ReasonforCreditClaim__c}, &apos;Late payment&apos;))</expression>
    </formulas>
    <formulas>
