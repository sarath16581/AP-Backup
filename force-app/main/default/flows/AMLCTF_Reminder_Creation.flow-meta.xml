    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&apos;Follow-Up with Retail Outlet: &apos; &amp; [Case].Network__c.Name &amp;  &apos; on actions for Audit Case: &apos; &amp; [Case].CaseNumber </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_3_myRule_1_A2_Subject</name>
        <dataType>String</dataType>
        <expression>&apos;Follow-Up with Retail Outlet: &apos; &amp; {!myVariable_current.Network__c.Name} &amp;  &apos; on actions for Audit Case: &apos; &amp; {!myVariable_current.CaseNumber}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF([Case].RecordType.DeveloperName = &apos;AMLCTF_Scheduled_Audit&apos; || [Case].RecordType.DeveloperName = &apos;AMLCTF_Assurance_Audit&apos;, 
TODAY() + $Setup.AMLCTF_Retail_Audit_Settings__c.Follow_Up_Interval_Site_Audit__c, IF([Case].RecordType.DeveloperName = &apos;AMLCTF_Please_Explain_Audit&apos;,
TODAY() + $Setup.AMLCTF_Retail_Audit_Settings__c.Follow_Up_Interval_Please_Explain__c,
null)) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_4_myRule_1_A1_Action_Date_c</name>
        <dataType>Date</dataType>
        <expression>IF({!myVariable_current.RecordType.DeveloperName} = &apos;AMLCTF_Scheduled_Audit&apos; || {!myVariable_current.RecordType.DeveloperName} = &apos;AMLCTF_Assurance_Audit&apos;, 
TODAY() + $Setup.AMLCTF_Retail_Audit_Settings__c.Follow_Up_Interval_Site_Audit__c, IF({!myVariable_current.RecordType.DeveloperName} = &apos;AMLCTF_Please_Explain_Audit&apos;,
TODAY() + $Setup.AMLCTF_Retail_Audit_Settings__c.Follow_Up_Interval_Please_Explain__c,
null))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(ISCHANGED([Case].Status), 
ISPICKVAL([Case].Status, &apos;Awaiting Information&apos;), 
OR([Case].RecordType.DeveloperName = &apos;AMLCTF_Please_Explain_Audit&apos;,
[Case].RecordType.DeveloperName = &apos;AMLCTF_Scheduled_Audit&apos;,
[Case].RecordType.DeveloperName = &apos;AMLCTF_Assurance_Audit&apos;,
[Case].RecordType.DeveloperName = &apos;AMLCTF_Unannounced_Audit&apos;) 
 )</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(ISCHANGED({!myVariable_current.Status}), 
ISPICKVAL({!myVariable_current.Status}, &apos;Awaiting Information&apos;), 
