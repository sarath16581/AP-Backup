    <description>This process will update case details once criterion was satisfied.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
        [Case].Case_RecordType_Name__c = &apos;Transit Warranty Claim&apos;,
        NOT([Case].Owner_is_Queue__c),
        OR(
              ISNEW(),
              ISCHANGED([Case].OwnerId)))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
        {!myVariable_current.Case_RecordType_Name__c} = &apos;Transit Warranty Claim&apos;,
        NOT({!myVariable_current.Owner_is_Queue__c}),
        OR(
              ISNEW(),
              ISCHANGED({!myVariable_current.OwnerId})))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
        ISCHANGED([Case].ArticleTest__c),
        [Case].Case_RecordType_Name__c = &apos;Delivery Enquiry&apos;,
        [Case].Calc_Owner_Name__c = &apos;Sorry We Missed You&apos;,
        ISPICKVAL([Case].Status, &apos;New&apos;), 
        [Case].ArticleTest__c &lt;&gt; null,
        NOT(AND(
              ISNULL([Case].Network__c),
              ISNULL([Case].ArticleTest__c.Network__c))),
        $Setup.StarTrack_Settings__c.ST_Automate_SWMY_Task_Creation__c,
        OR(
              ISPICKVAL([Case].Enquiry_Type__c, &apos;Check Address&apos;),
              ISPICKVAL([Case].Enquiry_Type__c, &apos;Depot Collection&apos;),
              ISPICKVAL([Case].Enquiry_Type__c, &apos;Redirection&apos;),
              ISPICKVAL([Case].Enquiry_Type__c, &apos;Redelivery&apos;)))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_6</name>
        <dataType>Boolean</dataType>
        <expression>AND(
        ISCHANGED({!myVariable_current.ArticleTest__c}),
        {!myVariable_current.Case_RecordType_Name__c} = &apos;Delivery Enquiry&apos;,
        {!myVariable_current.Calc_Owner_Name__c} = &apos;Sorry We Missed You&apos;,
        ISPICKVAL({!myVariable_current.Status}, &apos;New&apos;), 
        {!myVariable_current.ArticleTest__c} &lt;&gt; null,
        NOT(AND(
              ISNULL({!myVariable_current.Network__c}),
              ISNULL({!myVariable_current.ArticleTest__c.Network__c}))),
        $Setup.StarTrack_Settings__c.ST_Automate_SWMY_Task_Creation__c,
        OR(
              ISPICKVAL({!myVariable_current.Enquiry_Type__c}, &apos;Check Address&apos;),
              ISPICKVAL({!myVariable_current.Enquiry_Type__c}, &apos;Depot Collection&apos;),
              ISPICKVAL({!myVariable_current.Enquiry_Type__c}, &apos;Redirection&apos;),
              ISPICKVAL({!myVariable_current.Enquiry_Type__c}, &apos;Redelivery&apos;)))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
        [Case].StarTrack_RecordType__c,
        NOT(ISPICKVAL([Case].Status, &apos;Closed&apos;)),
        ISPICKVAL(PRIORVALUE([Case].Status), &apos;Reopened&apos;),
        ISCHANGED([Case].Status),
        [Case].All_Manual_Communications_Completed__c)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_8</name>
        <dataType>Boolean</dataType>
        <expression>AND(
        {!myVariable_current.StarTrack_RecordType__c},
        NOT(ISPICKVAL({!myVariable_current.Status}, &apos;Closed&apos;)),
        ISPICKVAL(PRIORVALUE({!myVariable_current.Status}), &apos;Reopened&apos;),
        ISCHANGED({!myVariable_current.Status}),
        {!myVariable_current.All_Manual_Communications_Completed__c})</expression>
    </formulas>
