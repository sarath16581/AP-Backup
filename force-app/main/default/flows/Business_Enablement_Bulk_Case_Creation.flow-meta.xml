    </recordLookups>
    <recordLookups>
        <name>Lookup_Schedule_Reports</name>
        <label>Lookup Schedule Reports</label>
        <locationX>384</locationX>
        <locationY>411</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Loop_through_Filtered_reports</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Active__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Report_Schedule__c</field>
            <operator>EqualTo</operator>
            <value>
