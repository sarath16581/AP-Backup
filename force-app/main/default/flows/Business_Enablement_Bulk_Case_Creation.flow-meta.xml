    <screens>
        <name>Select_Case_Creation_Date</name>
        <label>Select Case Creation Date</label>
        <locationX>300</locationX>
        <locationY>164</locationY>
        <allowBack>true</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>true</allowPause>
        <connector>
            <targetReference>Assign_Date</targetReference>
        </connector>
        <fields>
            <name>Enter_Date_for_Case_Creation</name>
            <dataType>Date</dataType>
            <fieldText>Enter Date for Case Creation</fieldText>
            <fieldType>InputField</fieldType>
            <helpText>Note: Custom created date cannot be in the future</helpText>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>Date cannot be in a future date</errorMessage>
                <formulaExpression>{!Enter_Date_for_Case_Creation} &lt; TODAY()</formulaExpression>
