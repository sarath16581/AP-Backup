    <decisions>
        <name>Case_Date_Decision</name>
        <label>Case Date Decision</label>
        <locationX>86</locationX>
        <locationY>164</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Current_Date_Time</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Set_Created_Date_as_to_current_date_time</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_RecordType_ID_Business_Enabement</targetReference>
            </connector>
            <label>Current Date  Time</label>
        </rules>
        <rules>
            <name>Adjusted_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Set_Created_Date_as_to_current_date_time</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Select_Case_Creation_Date</targetReference>
            </connector>
            <label>Adjusted Date</label>
        </rules>
    </decisions>
    <decisions>
        <name>Verify_Group_Membership</name>
        <label>Verify Group Membership</label>
        <locationX>383</locationX>
        <locationY>44</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Is_a_Group_Member</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varGroupMemberID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Business_Enablement_Bulk_Case_Creation</targetReference>
            </connector>
            <label>Is a Group Member</label>
        </rules>
        <rules>
            <name>Not_a_Group_Member</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varGroupMemberID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Not_Authorised</targetReference>
            </connector>
            <label>Not a Group Member</label>
        </rules>
    </decisions>
    <description>Flow created for the Startrack business Enablement team to bulk create cases n demand to track and log all scheduled reports that are required to be run and sent to customers periodically i.e. Daily, Weeekly, Monthly, Quarterly</description>
    <dynamicChoiceSets>
        <name>SchedulePicklist</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>Report_Schedule__c</picklistField>
        <picklistObject>Business_Enablement_Scheduled_Reports__c</picklistObject>
    </dynamicChoiceSets>
    <formulas>
        <name>CurrentUserID</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
    <interviewLabel>Business Enablement Bulk Case Creation {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Business Enablement Bulk Case Creation</label>
    <loops>
        <name>Loop_through_Filtered_reports</name>
        <label>Loop through Filtered reports</label>
        <locationX>608</locationX>
        <locationY>409</locationY>
        <assignNextValueToReference>SO_BusEnReports</assignNextValueToReference>
        <collectionReference>SOCV_ReportSchedule</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Map_Values_from_Report_to_Case</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_Cases</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processType>Flow</processType>
    <recordCreates>
        <name>Create_Cases</name>
        <label>Create Cases</label>
        <locationX>409</locationX>
        <locationY>532</locationY>
        <connector>
            <targetReference>Finnish_Screen</targetReference>
        </connector>
        <inputReference>SOCV_NewCaseList</inputReference>
    </recordCreates>
    <recordLookups>
        <name>Lookup_Current_User_Group_Membership</name>
        <label>Lookup Current User Group Membership</label>
        <locationX>626</locationX>
        <locationY>2</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Verify_Group_Membership</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>GroupId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varPublicGroupID</elementReference>
            </value>
        </filters>
        <filters>
            <field>UserOrGroupId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>CurrentUserID</elementReference>
            </value>
        </filters>
        <object>GroupMember</object>
        <outputAssignments>
            <assignToReference>varGroupMemberID</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Lookup_Public_Group_ID</name>
        <label>Lookup Public Group ID</label>
        <locationX>842</locationX>
        <locationY>46</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Lookup_Current_User_Group_Membership</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>DeveloperName</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varBusEnablmentAdminGroup</elementReference>
            </value>
        </filters>
        <object>Group</object>
        <outputAssignments>
            <assignToReference>varPublicGroupID</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Lookup_RecordType_ID_Business_Enabement</name>
        <label>Lookup RecordType ID Business Enabement</label>
        <locationX>89</locationX>
        <locationY>298</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Assign_Report_Schedule_Variable</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Name</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>conCaseRecordTypeName</elementReference>
            </value>
        </filters>
        <object>RecordType</object>
        <outputAssignments>
