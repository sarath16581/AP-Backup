            </value>
        </filters>
        <object>Business_Enablement_Scheduled_Reports__c</object>
        <outputReference>SOCV_ReportSchedule</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Account__c</queriedFields>
        <queriedFields>Report_Schedule__c</queriedFields>
        <queriedFields>Name</queriedFields>
        <queriedFields>Case_Recordtype_Name__c</queriedFields>
        <queriedFields>Default_Case_Owner__c</queriedFields>
        <queriedFields>Description__c</queriedFields>
        <queriedFields>Work_Type__c</queriedFields>
        <queriedFields>Support_Work_Type__c</queriedFields>
        <queriedFields>Status__c</queriedFields>
        <queriedFields>Time_to_Resolve_Hours__c</queriedFields>
        <sortField>Name</sortField>
        <sortOrder>Asc</sortOrder>
    </recordLookups>
