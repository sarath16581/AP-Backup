(ISPICKVAL({!myVariable_current.ResolutionCode__c}  , &apos;Assistance provided&apos;))

&amp;&amp; 

((ISNULL({!myVariable_current.Contact.Last_Survey_Date__c}) || ( NOT(ISNULL({!myVariable_current.Contact.Last_Survey_Date__c})) &amp;&amp; (Today() - {!myVariable_current.Contact.Last_Survey_Date__c}) &gt;=  7) ))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
$Setup.Business_Enablement_settings__c.EnableBusinessEnablementSurveys__c = TRUE,
[Case].RecordType.Name = &apos;Sales Coordinator&apos;,
ISPICKVAL([Case].Status, &apos;Closed&apos;),
ISCHANGED([Case].Status),
ISPICKVAL([Case].Request_Origin__c, &apos;On Demand&apos;),
NOT(ISBLANK([Case].Requestor__c)),
OR(
	ISNULL([Case].Requestor__c.Last_Business_Enablement_Survey_Date__c),
	Now() - ([Case].Requestor__c.Last_Business_Enablement_Survey_Date__c) &gt;= $Setup.Business_Enablement_settings__c.MinofDaysbetweenSurveys__c 
	)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!$Setup.Business_Enablement_settings__c.EnableBusinessEnablementSurveys__c} = TRUE,
