ISPICKVAL({!myVariable_current.Status}, &apos;Closed&apos;),
ISCHANGED({!myVariable_current.Status}),
ISPICKVAL({!myVariable_current.Request_Origin__c}, &apos;On Demand&apos;),
NOT(ISBLANK({!myVariable_current.Requestor__c})),
OR(
