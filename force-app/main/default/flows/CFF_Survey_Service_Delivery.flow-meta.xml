    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>
[Case].RecordType.Name = &apos;Service Delivery&apos; &amp;&amp; 

ISPICKVAL([Case].Status, &apos;Closed&apos;) &amp;&amp; 

(ISPICKVAL([Case].Origin, &apos;Customer - Email&apos;) || ISPICKVAL([Case].Origin, &apos;Customer - Phone&apos;) || ISPICKVAL([Case].Origin, &apos;Customer - In Person&apos;)) &amp;&amp;  

(ISPICKVAL([Case].ResolutionCode__c  , &apos;Assistance provided&apos;))

&amp;&amp; 

((ISNULL([Case].Contact.Last_Survey_Date__c) || ( NOT(ISNULL([Case].Contact.Last_Survey_Date__c)) &amp;&amp; (Today() - [Case].Contact.Last_Survey_Date__c) &gt;=  7) ))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>
{!myVariable_current.RecordType.Name} = &apos;Service Delivery&apos; &amp;&amp; 

