    <hasActivationRequired>false</hasActivationRequired>
    <label>Set Audit Fields</label>
    <license>Salesforce</license>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>UpdateWithInactiveOwner</name>
    </userPermissions>
</PermissionSet>
